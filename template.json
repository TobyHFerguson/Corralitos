{
    "provisioners": [
	{
	    "type": "shell",
	    "execute_command": "echo 'vagrant'|sudo -S sh '{{.Path}}'",
	    "override": {
		"virtualbox-iso": {
		    "scripts": [
			"scripts/base.sh",
			"scripts/vagrant.sh",
			"scripts/virtualbox.sh",
			"scripts/cleanup.sh"
		    ]
		}
	    }
	}
    ],
    "post-processors": [
	{
	    "type": "vagrant",
	    "override": {
		"virtualbox": {
		    "output": "ol6u5-minimal-btrfs-uek.box"
		}
	    }
	}
    ],
    "builders": [
	{
	    "type": "virtualbox-iso",
	    "boot_command": [
		"<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/kickstart/ks.cfg repo=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ol6<enter><wait>"
	    ],
	    "boot_wait": "10s",
	    "disk_size": 102400,
	    "guest_os_type": "Oracle_64",
	    "http_directory": "http",
            "iso_checksum": "43cbce2427f3986a3614d6997dd742b3",
	    "iso_checksum_type": "md5",
	    "iso_url": "file:///net/qnap1/Public/ISOs/OL6U5-X86_64-UEK-Boot-V41364-01.iso",
	    "ssh_username": "vagrant",
	    "ssh_password": "vagrant",
	    "ssh_port": 22,
	    "ssh_wait_timeout": "10000s",
	    "shutdown_command": "echo '/sbin/halt -h -p' > /tmp/shutdown.sh; echo 'vagrant'|sudo -S sh '/tmp/shutdown.sh'",
	    "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
	    "virtualbox_version_file": ".vbox_version",
	    "vboxmanage": [
		[ "modifyvm", "{{.Name}}", "--memory", "1024" ],
		[ "modifyvm", "{{.Name}}", "--cpus", "1" ]
	    ],
	    "vm_name": "ol6u5-minimal-btrfs-uek"
	}
    ]
}
